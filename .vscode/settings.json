{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.format.settings.url": "studdybuddy/config/checkstyle/eclipse-java-google-style.xml",
    "[java]": {
        "editor.tabSize": 2
    },
    "java.test.config": [
        {
            "name": "coreTesting",
            "vmArgs": []
        },
        {
            "name": "uiTesting",
            "vmArgs": ["--add-opens", "ui/ui=ALL-UNNAMED", "--add-exports", "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"]
        }
    ],
    "java.test.defaultConfig": "uiTesting"

}