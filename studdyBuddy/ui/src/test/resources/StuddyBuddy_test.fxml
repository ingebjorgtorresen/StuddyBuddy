<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.StuddyBuddyController" prefHeight="640.0" prefWidth="350.0">
    <children>
        <Pane prefHeight="640.0" prefWidth="350"> 
            <children>
                <Label text="Name:" layoutX="10.0" layoutY="200.0"/>

                <TextField fx:id="nameField" promptText="Username" layoutX="60.0" layoutY="200.0"/>
                    
                <Button text="Login" onAction="#handleLogin" layoutX="170.0" layoutY="300.0"/>

                <Label text="" fx:id="errorMessage" layoutX="20.0" layoutY="450.0"/>
            <children>
        <Pane>
    <children>
</AnchorPane>